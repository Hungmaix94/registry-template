{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "contact",
  "type": "registry:component",
  "title": "Contact Section",
  "description": "A section with contact information and a form.",
  "registryDependencies": [
    "button",
    "card",
    "input",
    "label",
    "textarea"
  ],
  "files": [
    {
      "path": "registry/default-style/blocks/contact/contact.tsx",
      "content": "import { Button } from \"@/components/ui/button\"\nimport { Card, CardContent, CardHeader, CardTitle } from \"@/components/ui/card\"\nimport { Input } from \"@/components/ui/input\"\nimport { Label } from \"@/components/ui/label\"\nimport { Textarea } from \"@/components/ui/textarea\"\nimport { cn } from \"@/lib/utils\"\n\ninterface ContactInfo {\n  address?: string;\n  phone?: string;\n  email?: string;\n}\n\ninterface ContactProps extends React.HTMLAttributes<HTMLDivElement> {\n  sectionTitle: string;\n  sectionDescription?: string;\n  contactInfo?: ContactInfo;\n  variant?: \"basic-form\" | \"form-with-info\" | \"form-with-map\";\n}\n\nexport default function Contact({ sectionTitle, sectionDescription, contactInfo, variant = \"basic-form\", className, ...props }: ContactProps) {\n  return (\n    <section className={cn(\"w-full py-12 md:py-24 lg:py-32\", className)} {...props}>\n      <div className=\"container px-4 md:px-6\">\n        <div className=\"flex flex-col items-center justify-center space-y-4 text-center mb-8\">\n          <div className=\"space-y-2\">\n            <h2 className=\"text-3xl font-bold tracking-tighter sm:text-5xl\">{sectionTitle}</h2>\n            {sectionDescription && <p className=\"max-w-[900px] text-muted-foreground md:text-xl/relaxed lg:text-base/relaxed xl:text-xl/relaxed\">{sectionDescription}</p>}\n          </div>\n        </div>\n\n        <div className=\"grid gap-8 md:grid-cols-2 lg:grid-cols-3\">\n          {(variant === \"form-with-info\" || variant === \"form-with-map\") && contactInfo && (\n            <Card className=\"flex flex-col p-6\">\n              <CardHeader>\n                <CardTitle>Contact Information</CardTitle>\n              </CardHeader>\n              <CardContent className=\"space-y-4\">\n                {contactInfo.address && (\n                  <div className=\"flex items-start gap-2\">\n                    <MapPinIcon className=\"h-5 w-5 text-muted-foreground\" />\n                    <p>{contactInfo.address}</p>\n                  </div>\n                )}\n                {contactInfo.phone && (\n                  <div className=\"flex items-center gap-2\">\n                    <PhoneIcon className=\"h-5 w-5 text-muted-foreground\" />\n                    <p>{contactInfo.phone}</p>\n                  </div>\n                )}\n                {contactInfo.email && (\n                  <div className=\"flex items-center gap-2\">\n                    <MailIcon className=\"h-5 w-5 text-muted-foreground\" />\n                    <p>{contactInfo.email}</p>\n                  </div>\n                )}\n              </CardContent>\n            </Card>\n          )}\n\n          <Card className={cn(\"p-6\", (variant === \"basic-form\" || variant === \"form-with-info\") ? \"md:col-span-1 lg:col-span-1\" : \"md:col-span-2 lg:col-span-2\")}>\n            <CardHeader>\n              <CardTitle>Send Us a Message</CardTitle>\n            </CardHeader>\n            <CardContent className=\"space-y-4\">\n              <div className=\"grid gap-2\">\n                <Label htmlFor=\"name\">Name</Label>\n                <Input id=\"name\" placeholder=\"Your Name\" />\n              </div>\n              <div className=\"grid gap-2\">\n                <Label htmlFor=\"email\">Email</Label>\n                <Input id=\"email\" type=\"email\" placeholder=\"your@example.com\" />\n              </div>\n              <div className=\"grid gap-2\">\n                <Label htmlFor=\"subject\">Subject</Label>\n                <Input id=\"subject\" placeholder=\"Subject\" />\n              </div>\n              <div className=\"grid gap-2\">\n                <Label htmlFor=\"message\">Message</Label>\n                <Textarea id=\"message\" placeholder=\"Your Message\" rows={5} />\n              </div>\n              <Button type=\"submit\" className=\"w-full\">\n                Send Message\n              </Button>\n            </CardContent>\n          </Card>\n\n          {variant === \"form-with-map\" && (\n            <Card className=\"flex flex-col p-6 md:col-span-1 lg:col-span-1\">\n              <CardHeader>\n                <CardTitle>Our Location</CardTitle>\n              </CardHeader>\n              <CardContent className=\"h-64 w-full\">\n                {/* Placeholder for map iframe or image */}\n                <iframe\n                  src=\"https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3153.2084042858567!2d144.9630579153185!3d-37.81627997975149!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x6ad642af0f11fd81%3A0xf5771d9d714b1a40!2sFederation%20Square!5e0!3m2!1sen!2sau!4v1678888888888!5m2!1sen!2sau\"\n                  width=\"100%\"\n                  height=\"100%\"\n                  style={{ border: 0 }}\n                  allowFullScreen\n                  loading=\"lazy\"\n                  referrerPolicy=\"no-referrer-when-downgrade\"\n                ></iframe>\n              </CardContent>\n            </Card>\n          )}\n        </div>\n      </div>\n    </section>\n  );\n}\n\n// Basic icons for illustrative purposes\nfunction MapPinIcon(props: React.SVGProps<SVGSVGElement>) {\n  return (\n    <svg\n      {...props}\n      xmlns=\"http://www.w3.org/2000/svg\"\n      width=\"24\"\n      height=\"24\"\n      viewBox=\"0 0 24 24\"\n      fill=\"none\"\n      stroke=\"currentColor\"\n      strokeWidth=\"2\"\n      strokeLinecap=\"round\"\n      strokeLinejoin=\"round\"\n    >\n      <path d=\"M20 10c0 6-8 12-8 12s-8-6-8-12a8 8 0 0 1 16 0Z\" />\n      <circle cx=\"12\" cy=\"10\" r=\"3\" />\n    </svg>\n  )\n}\n\nfunction PhoneIcon(props: React.SVGProps<SVGSVGElement>) {\n  return (\n    <svg\n      {...props}\n      xmlns=\"http://www.w3.org/2000/svg\"\n      width=\"24\"\n      height=\"24\"\n      viewBox=\"0 0 24 24\"\n      fill=\"none\"\n      stroke=\"currentColor\"\n      strokeWidth=\"2\"\n      strokeLinecap=\"round\"\n      strokeLinejoin=\"round\"\n    >\n      <path d=\"M22 16.92v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z\" />\n    </svg>\n  )\n}\n\nfunction MailIcon(props: React.SVGProps<SVGSVGElement>) {\n  return (\n    <svg\n      {...props}\n      xmlns=\"http://www.w3.org/2000/svg\"\n      width=\"24\"\n      height=\"24\"\n      viewBox=\"0 0 24 24\"\n      fill=\"none\"\n      stroke=\"currentColor\"\n      strokeWidth=\"2\"\n      strokeLinecap=\"round\"\n      strokeLinejoin=\"round\"\n    >\n      <rect width=\"20\" height=\"16\" x=\"2\" y=\"4\" rx=\"2\" />\n      <path d=\"m22 7-8.97 5.79a1.94 1.94 0 0 1-2.06 0L2 7\" />\n    </svg>\n  )\n}\n",
      "type": "registry:component"
    },
    {
      "path": "registry/default-style/blocks/contact/mock-data.ts",
      "content": "export const mockContactData = {\n  sectionTitle: \"Get in Touch\",\n  sectionDescription: \"Have questions, feedback, or just want to say hello? Reach out to us using the form below or our contact details.\",\n  contactInfo: {\n    address: \"123 Main Street, Anytown, CA 90210\",\n    phone: \"+1 (555) 123-4567\",\n    email: \"info@example.com\",\n  },\n};\n",
      "type": "registry:lib"
    }
  ]
}